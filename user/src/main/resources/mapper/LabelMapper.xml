<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zzy.mapper.LabelMapper">

    <resultMap id="BaseResultMap" type="com.zzy.domain.Label">
            <id property="labelId" column="label_id" jdbcType="INTEGER"/>
            <result property="labelName" column="label_name" jdbcType="VARCHAR"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
            <result property="isDelete" column="is_delete" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        label_id,label_name,create_time,
        update_time,is_delete
    </sql>
</mapper>
